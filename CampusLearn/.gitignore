assets/js/env.js

# .env files (if any future Node or backend work)
.env
.env.*
.env.local
.env.development.local
.env.production.local

##########################
# 2. OS Files
##########################
# macOS
.DS_Store
.AppleDouble
.LSOverride

# Windows
Thumbs.db
ehthumbs.db
Desktop.ini

##########################
# 3. Editor Settings
##########################
# VS Code
.vscode/
.history/
*.code-workspace

# JetBrains / Visual Studio
.idea/
.vs/
*.suo
*.user
*.userosscache
*.sln.docstates

##########################
# 4. Build / Cache / Temp
##########################
# Node-related (if you add npm later)
node_modules/
dist/
build/
.cache/
.next/
out/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Other tools
coverage/
tmp/
temp/
*.tmp

##########################
# 5. Compiled / Generated Files
##########################
*.exe
*.dll
*.pdb
*.obj
*.o
*.class
*.jar
*.pyc
__pycache__/
*.out

##########################
# 6. Local Database / IDE Data
##########################
*.sqlite3
*.db
*.bak
*.mdf
*.ldf

##########################
# 7. Miscellaneous
##########################
# Archives & uploads
*.zip
*.tar
*.gz
*.rar

# System logs or OS metadata
*.pid
*.seed
*.pid.lock

##########################
# 8. Project-specific extras
##########################
# Don’t commit local test data
resources_seed.csv

# Ignore temporary SQL exports
supabase/sql/*.backup
supabase/sql/*.tmp
supabase/sql/*.log

# Ignore Live Server settings
*.live-server.json

